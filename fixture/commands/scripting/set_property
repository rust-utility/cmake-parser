set_property(GLOBAL PROPERTY property1)
set_property(GLOBAL APPEND APPEND_STRING PROPERTY property1 value1 value2)
set_property(DIRECTORY PROPERTY property1)
set_property(DIRECTORY dir1 APPEND APPEND_STRING PROPERTY property1 value1 value2)
set_property(TARGET PROPERTY property1)
set_property(TARGET target1 target2 PROPERTY property1)
set_property(SOURCE PROPERTY property1)
set_property(SOURCE src1 src2 PROPERTY property1)
set_property(SOURCE DIRECTORY dir1 dir2 PROPERTY property1)
set_property(SOURCE TARGET_DIRECTORY tdir1 tdir2 PROPERTY property1)
set_property(SOURCE DIRECTORY dir1 dir2 TARGET_DIRECTORY tdir1 tdir2 PROPERTY property1)
set_property(SOURCE src1 src2 DIRECTORY dir1 dir2 TARGET_DIRECTORY tdir1 tdir2 APPEND APPEND_STRING PROPERTY property1 value1 value2)
set_property(INSTALL PROPERTY property1)
set_property(INSTALL install1 install2 PROPERTY property1)
set_property(TEST PROPERTY property1)
set_property(TEST test1 test2 PROPERTY property1)
set_property(CACHE PROPERTY property1)
set_property(CACHE entry1 entry2 PROPERTY property1)
